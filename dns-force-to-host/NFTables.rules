table NAT {

  chain prerouting {
    type nat hook prerouting priority 0
    tcp dport   53 ip saddr 192.168.1.0/24 dnat 192.168.1.1:53
    udp dport   53 ip saddr 192.168.1.0/24 dnat 192.168.1.1:53
    tcp dport  853 ip saddr 192.168.1.0/24 dnat 192.168.1.1:853
    udp dport  853 ip saddr 192.168.1.0/24 dnat 192.168.1.1:853
    tcp dport 5353 ip saddr 192.168.1.0/24 dnat 192.168.1.1:5353
    udp dport 5353 ip saddr 192.168.1.0/24 dnat 192.168.1.1:5353
  }

}

